# Este es un comentario al inicio de la línea
mode: quiet # Comentario al final de la línea
- power_limit: 80 # Otro comentario
- fan_speed: 30
- clocks: 100 # Comentario explicativo
- persist: on

# Configuración para modo balanceado
mode: balanced
- power_limit: 100
- fan_speed: 50 # Velocidad del ventilador
- clocks: 500
- persist: off

# Configuración de alto rendimiento
mode: performance
- power_limit: 130
- fan_speed: 80
- clocks: 1800 # Frecuencia máxima
- persist: on

status # Mostrar estado final 